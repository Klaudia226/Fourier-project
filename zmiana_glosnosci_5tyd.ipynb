   "source": [
    "using WAV\n",
    "using FFTW"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "wavplay(\"440Hz.wav\")\n",
    "sound = wavread(\"440Hz.wav\") \n",
    "sound_fft = 0.1 .* fft(sound[1][:, 1])\n",
    "sound_ifft = real.(ifft(sound_fft))\n",
    "wavplay(sound_ifft, sound[2])\n",
    "wavplay(\"440Hz.wav\")"
   ]
  },
